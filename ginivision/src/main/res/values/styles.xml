<resources>

    <!-- Base application theme. -->
    <style name="GiniVisionTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/gv_colorPrimary</item>
        <item name="colorPrimaryDark">@color/gv_colorPrimaryDark</item>
        <item name="colorAccent">@color/gv_colorAccent</item>
    </style>

    <style name="GiniVisionTheme.Transparent">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>

</resources>
