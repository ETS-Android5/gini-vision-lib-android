<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.gini.android.ginivisiontest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="net.gini.android.vision.easy.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="net.gini.android.vision.easy.ReviewDocumentActivity"
            android:label="@string/gv_title_review_document" />
        <activity
            android:name="net.gini.android.vision.easy.AnalysisActivity"
            android:label="@string/gv_title_analyze_document" />
    </application>

</manifest>