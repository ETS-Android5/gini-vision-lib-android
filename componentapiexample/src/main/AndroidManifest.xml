<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.gini.android.visionadvtest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="net.gini.android.vision.component.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="net.gini.android.vision.component.CustomCameraActivity"
            android:label="@string/title_scanner" />
        <activity
            android:name="net.gini.android.vision.component.CustomCameraAppCompatActivity"
            android:label="@string/title_scanner"
            android:theme="@style/AppThemeCompat" />
        <activity
            android:name="net.gini.android.vision.component.CustomOnboardingActivity"
            android:label="@string/title_onboarding" />
        <activity
            android:name="net.gini.android.vision.component.CustomOnboardingAppCompatActivity"
            android:label="@string/title_onboarding"
            android:theme="@style/AppThemeCompat" />
        <activity
            android:name="net.gini.android.vision.component.CustomReviewActivity"
            android:label="@string/title_review" />
        <activity
            android:name="net.gini.android.vision.component.CustomReviewAppCompatActivity"
            android:label="@string/title_review"
            android:theme="@style/AppThemeCompat" />
        <activity
            android:name="net.gini.android.vision.component.CustomAnalysisActivity"
            android:label="@string/title_analysis" />
        <activity
            android:name="net.gini.android.vision.component.CustomAnalysisAppCompatActivity"
            android:label="@string/title_analysis"
            android:theme="@style/AppThemeCompat" />
    </application>

</manifest>